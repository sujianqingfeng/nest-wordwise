{
  "name": "nest-wordwise",
  "version": "1.0.0",
  "private": true,
  "description": "wordwise-backend",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "drizzle-kit generate:pg",
    "migrate": "drizzle-kit push:pg",
    "migrate:prod": "drizzle-kit push:pg --config drizzle.prod.config.ts",
    "studio": "drizzle-kit studio",
    "dict:write": "node scripts/ecdict/write.mjs",
    "dev:db": "sudo docker-compose up",
    "deploy": "pnpm build && vercel --prod"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@volcengine/openapi": "^1.11.2",
    "axios": "^1.5.1",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.30.0",
    "drizzle-orm": "^0.29.3",
    "http-proxy-middleware": "^2.0.6",
    "jsdom": "^23.0.1",
    "nestjs-zod": "^3.0.0",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@sujian/eslint-config": "^1.0.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.12",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.10",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vercel": "^33.0.2"
  }
}
