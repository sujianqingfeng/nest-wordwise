{
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "json",
    "jsonc",
    "json5"
  ],
  "cSpell.words": [
    "ecdict",
    "volcengine",
    "wordwise"
  ]
}